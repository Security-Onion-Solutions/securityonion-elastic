. /usr/sbin/so-common
. /usr/lib/nsmnow/lib-console-utils
. /etc/nsm/securityonion.conf

function ok(){
        echo -e "$STAT[${GREEN_BOLD}  OK  ${NORMAL}]"
}
function fail(){
        echo -e "$STAT[${RED_BOLD} FAIL ${NORMAL}]"
}
function warn(){
        echo -e "$STAT[${YELLOW_BOLD} WARN ${NORMAL}]"
}
